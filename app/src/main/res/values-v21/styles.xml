<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#000022</item>
        <item name="colorPrimaryDark">#000011</item>
        <item name="colorAccent">#ffffff</item>
    </style>

    <style name="NoActionBar" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
        <item name="colorAccent">#01d277</item>
        <item name="android:navigationBarColor">#000000</item>
        <item name="android:statusBarColor">#212121</item>
    </style>

    <style name="NoActionBar2" parent="Theme.AppCompat.NoActionBar">
        <item name="colorAccent">#01d277</item>
        <item name="android:navigationBarColor">#000000</item>
        <item name="android:statusBarColor">#212121</item>
    </style>

</resources>